{
  "name": "walletchan",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:extension": "pnpm --filter @walletchan/extension dev",
    "dev:website": "pnpm --filter @walletchan/website dev",
    "build": "pnpm build:extension && pnpm build:website",
    "build:extension": "pnpm --filter @walletchan/extension build",
    "build:website": "pnpm --filter @walletchan/website build",
    "lint": "pnpm --filter @walletchan/extension lint",
    "zip": "pnpm --filter @walletchan/extension zip",
    "zip:cws": "pnpm --filter @walletchan/extension zip:cws",
    "dev:indexer": "pnpm --filter @walletchan/indexer dev",
    "start:indexer": "pnpm --filter @walletchan/indexer start",
    "dev:staking-indexer": "pnpm --filter @walletchan/staking-indexer dev",
    "start:staking-indexer": "pnpm --filter @walletchan/staking-indexer start",
    "dev:fee-indexer": "pnpm --filter @walletchan/fee-indexer dev",
    "start:fee-indexer": "pnpm --filter @walletchan/fee-indexer start",
    "dev:wchan-vault-indexer": "pnpm --filter @walletchan/wchan-vault-indexer dev",
    "start:wchan-vault-indexer": "pnpm --filter @walletchan/wchan-vault-indexer start",
    "dev:tg-bot": "pnpm --filter @walletchan/tg-bot dev",
    "start:tg-bot": "pnpm --filter @walletchan/tg-bot start",
    "build:tg-bot": "pnpm --filter @walletchan/tg-bot build",
    "dev:arb-bot": "pnpm --filter @walletchan/arb-bot dev",
    "start:arb-bot": "pnpm --filter @walletchan/arb-bot start",
    "build:arb-bot": "pnpm --filter @walletchan/arb-bot build",
    "icons": "pnpm --filter @walletchan/extension icons",
    "release:patch": "pnpm --filter @walletchan/extension release:patch",
    "release:minor": "pnpm --filter @walletchan/extension release:minor",
    "release:major": "pnpm --filter @walletchan/extension release:major",
    "build:contracts": "pnpm --filter @walletchan/contracts build",
    "test:contracts": "pnpm --filter @walletchan/contracts test",
    "sync:addresses": "pnpm dlx tsx apps/website/scripts/sync-addresses.ts"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0"
}
